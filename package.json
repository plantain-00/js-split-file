{
  "name": "js-split-file",
  "version": "2.0.0",
  "description": "A library to split big file to small binary data for nodejs and browsers.",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc -p src/tsconfig.nodejs.json && tsc -p src/tsconfig.browser.json",
    "build": "npm run clean && npm run tsc && npm run rollup",
    "rollup": "rollup --config rollup.config.js",
    "lint": "npm run tslint && npm run standard",
    "tslint": "tslint \"*.ts\" \"spec/*.ts\"",
    "test": "tsc -p spec && jasmine",
    "release": "clean-release",
    "standard": "standard \"**/*.config.js\"",
    "fix": "standard --fix \"**/*.config.js\"",
    "clean": "rimraf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/js-split-file.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/js-split-file/issues"
  },
  "homepage": "https://github.com/plantain-00/js-split-file#readme",
  "dependencies": {
    "fluent-binary-converter": "2",
    "tslib": "1"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.53",
    "@types/node": "8.0.7",
    "clean-release": "1.3.1",
    "jasmine": "2.6.0",
    "rimraf": "2.6.1",
    "rollup": "0.45.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "standard": "10.0.2",
    "tslint": "5.4.3",
    "typescript": "2.4.1"
  }
}
